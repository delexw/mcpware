{
  "backends": [
    {
      "name": "github",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "description": "GitHub MCP Server",
      "timeout": 30
    },
    {
      "name": "time",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "mcp/time"
      ],
      "description": "Time MCP Server",
      "timeout": 30
    }
  ]
}